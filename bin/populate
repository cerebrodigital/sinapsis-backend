#!/usr/bin/env node

/**
 * Module dependencies.
 */
var models = require("../models")

let seeds = [
  "mocks",
  "post_types",
  "vote_types",
  "action_types",
  "categories",
  ]

models.sequelize.sync().then(function () {
  seeds.forEach(seed=>{
    require(`../seeds/${seed}`)(models)
  })
})

