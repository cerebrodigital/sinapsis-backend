#!/usr/bin/env node

/**
 * Module dependencies.
 */
var models = require("../models")
var mocks = require("../seeds/mocks")
var post_types = require("../seeds/post_types")
var vote_types = require("../seeds/vote_types")
var actions_types = require("../seeds/action_types")

models.sequelize.sync().then(function () {
  mocks(models)
  post_types(models)
  vote_types(models)
  action_types(models)
});

